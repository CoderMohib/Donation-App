{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk",
        "prebuildCommand": "sh scripts/eas-build-pre-install.sh"
      },
      "env": {
        "GOOGLE_SERVICES_JSON": "./google-services.json"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
